# testing data: sample_input1, master: fcntl, slave: fcntl
sudo ./master 10 ../sample_input_1/target_file_1 ../sample_input_1/target_file_2 ../sample_input_1/target_file_3 \
../sample_input_1/target_file_4 ../sample_input_1/target_file_5 ../sample_input_1/target_file_6 \
../sample_input_1/target_file_7 ../sample_input_1/target_file_8 ../sample_input_1/target_file_9 \
../sample_input_1/target_file_10 fcntl >> log/master/test1_ff &
sudo ./slave 10 ../sample_output_1/target_file_1 ../sample_output_1/target_file_2 ../sample_output_1/target_file_3 \
../sample_output_1/target_file_4 ../sample_output_1/target_file_5 ../sample_output_1/target_file_6 \
../sample_output_1/target_file_7 ../sample_output_1/target_file_8 ../sample_output_1/target_file_9 \
../sample_output_1/target_file_10 fcntl 127.0.0.1  >> log/slave/test1_ff
wait

diff ../sample_input_1/target_file_1 ../sample_output_1/target_file_1
diff ../sample_input_1/target_file_2 ../sample_output_1/target_file_2
diff ../sample_input_1/target_file_3 ../sample_output_1/target_file_3
diff ../sample_input_1/target_file_4 ../sample_output_1/target_file_4
diff ../sample_input_1/target_file_5 ../sample_output_1/target_file_5
diff ../sample_input_1/target_file_6 ../sample_output_1/target_file_6
diff ../sample_input_1/target_file_7 ../sample_output_1/target_file_7
diff ../sample_input_1/target_file_8 ../sample_output_1/target_file_8
diff ../sample_input_1/target_file_9 ../sample_output_1/target_file_9
diff ../sample_input_1/target_file_10 ../sample_output_1/target_file_10
