for f in ../sample_input_1/*; do
    echo transfering ${f##*/}...
    sudo ./master 1 ../sample_input_1/${f##*/} fcntl > log/master_${f##*/} &
    sudo ./slave 1 ../sample_output_1/${f##*/} fcntl 127.0.0.1 > log/slave_${f##*/} &
    wait
done

for f in ../sample_input_1/*; do
    echo matching ${f##*/}
    diff ../sample_input_1/${f##*/} ../sample_output_1/${f##*/}
done

